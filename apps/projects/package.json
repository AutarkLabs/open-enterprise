{
  "name": "@tps/apps-projects",
  "version": "0.0.1",
  "description": "Project management app from Planning Suite",
  "scripts": {
    "build:app": "parcel build app/index.html -d dist/ --public-url '.'",
    "build:script": "parcel build app/script.js -d dist/",
    "build": "npm run compile && npm run sync-assets && npm run build:app && npm run build:script",
    "compile": "truffle compile",
    "console": "truffle dev",
    "coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "dev": "npm run sync-assets && parcel app/index.html --port 3333",
    "ganache-cli:test": "../shared/test-helpers/ganache-cli.sh",
    "lint": "solium --dir ./contracts",
    "prepublishOnly": "truffle compile --all",
    "publish:http": "npm run publish:http-copy-files && aragon apm publish patch --http localhost:3333 --http-served-from ./dist",
    "publish:http-copy-files": "npm run build:script && mkdir -p dist/images && cp images/icon.svg dist/images/",
    "publish": "aragon apm publish major && sleep 3",
    "publish:major": "aragon apm publish major",
    "deploy:rinkeby": "npm run publish:major -- --environment staging",
    "start": "aragon run",
    "sync-assets": "copy-aragon-ui-assets ./dist",
    "test:gas": "GAS_REPORTER=true npm test",
    "test": "TRUFFLE_TEST=true npm run ganache-cli:test"
  },
  "dependencies": {
    "@aragon/client": "1.1.0",
    "@aragon/ui": "0.30.1",
    "@aragon/apps-vault": "^4.0.0",
    "apollo-boost": "^0.1.22",
    "graphql": "^14.0.2",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.0",
    "ipfs-http-client": "^29.1.0",
    "prop-types": "15.6.2",
    "react": "^16.7.0",
    "react-apollo": "^2.3.2",
    "react-dom": "^16.7.0",
    "react-number-format": "3.5.2",
    "rxjs": "6.3.2",
    "rxjs-compat": "6.3.2",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@tps/test-helpers": "^0.0.1",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "eslint": "5.5.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-react": "7.11.1",
    "parcel-bundler": "1.9.7",
    "prettier": "1.14.2",
    "react-hot-loader": "4.3.6",
    "stylelint": "9.5.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.2"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/AutarkLabs/planning-suite"
  },
  "keywords": [],
  "license": "GPL-3.0"
}
