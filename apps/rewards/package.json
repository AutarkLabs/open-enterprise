{
  "name": "@tps/apps-rewards",
  "version": "0.0.1",
  "description": "Rewards app from Planning Suite",
  "scripts": {
    "build:app": "parcel build app/index.html -d dist/ --public-url \".\" --no-cache",
    "build:script": "parcel build app/script.js -d dist/ --no-cache",
    "build": "npm run compile && npm run sync-assets && npm run build:app && npm run build:script",
    "compile": "truffle compile",
    "console": "truffle dev",
    "copy-public-assets": "rsync -rtu ./public/ ./dist",
    "coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "dev": "npm run sync-assets && parcel app/index.html --port 5555",
    "ganache-cli:test": "sh ../../node_modules/@aragon/test-helpers/ganache-cli.sh",
    "lint": "solium --dir ./contracts",
    "prepublishOnly": "truffle compile --all",
    "publish:http": "npm run build:script && aragon apm publish major --http localhost:5555 --http-served-from ./dist",
    "publish": "aragon apm publish patch && sleep 3",
    "publish:major": "aragon apm publish major --files dist/",
    "publish:minor": "aragon apm publish minor --files dist/",
    "publish:patch": "aragon apm publish patch --files dist/",
    "deploy:rinkeby": "npm run publish:major -- --environment rinkeby",
    "deploy:rpc": "npm run publish:patch -- --environment default",
    "deploy:staging": "npm run publish:major -- --environment staging",
    "start": "aragon run",
    "sync-assets": "copy-aragon-ui-assets -n aragon-ui ./dist && npm run copy-public-assets",
    "test:gas": "GAS_REPORTER=true npm test",
    "test": "cross-env TRUFFLE_TEST=true npm run ganache-cli:test"
  },
  "dependencies": {
    "@aragon/api": "1.1.0",
    "@aragon/api-react": "1.0.0-beta.2",
    "@aragon/apps-shared-minime": "1.0.1",
    "@aragon/apps-vault": "4.1.0",
    "@aragon/ui": "0.33.0",
    "@aragon/os": "4.2.0",
    "@babel/polyfill": "^7.2.5",
    "bignumber.js": "^7.2.1",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-blockies": "1.3.0",
    "react-dom": "^16.8.6",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@aragon/cli": "5.6.2",
    "@aragon/test-helpers": "^1.0.1",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "eth-gas-reporter": "0.1.12",
    "ethereumjs-abi": "0.6.5",
    "ethlint": "^1.2.4",
    "ganache-cli": "6.1.8",
    "homedir": "^0.6.0",
    "parcel-bundler": "1.12.3",
    "prettier": "^1.17.1",
    "react-hot-loader": "^4.8.8",
    "solidity-coverage": "0.5.11",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.7.0",
    "truffle": "4.1.14",
    "truffle-hdwallet-provider-privkey": "0.3.0",
    "truffle-privatekey-provider": "1.1.0"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/AutarkLabs/planning-suite"
  },
  "keywords": [],
  "license": "GPL-3.0"
}
